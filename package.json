    "postcss": "^7.0.32",
    "postcss-loader": "^4.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^2.2.1 || ^3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures",
    "rules": {
      "@typescript-eslint/no-namespace": "off"
