    "@types/terser-webpack-plugin": "3.0.0",
    "@types/webpack": "4.41.22",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.8.1",
    "prettier": "2.1.1",
    "semantic-release": "17.1.1",
    "typescript": "4.0.2"
  },
  "peerDependencies": {
    "ts-loader": "^5.3.2 || ^6.0.0 || ^8.0.0",
    "typescript": "^3.2.2 || ^4.0.0",
