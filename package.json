{
  "name": "@softwareventures/webpack-config",
  "version": "0.0.0-development",
  "description": "Standard webpack configuration for Software Ventures Limited",
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/webpack-config",
  "bugs": "https://github.com/softwareventures/webpack-config/issues",
  "repository": "github:softwareventures/webpack-config",
  "license": "ISC",
  "scripts": {
    "fix": "tsc --noEmit && eslint . --fix && prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check .",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "echo No tests yet"
  },
  "sideEffects": false,
  "engines": {
    "node": "^12 || ^14 || >=16"
  },
  "dependencies": {
    "@softwareventures/array": "4.1.3",
    "@softwareventures/dictionary": "^0.7.1 || ^0.8.0 || ^1.0.0",
    "@softwareventures/tsconfig": "^6.0.0-alpha.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.2 || ^6.0.0",
    "cssnano": "^4.1.10 || ^5.0.0",
    "ejs-loader": "^0.5.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-minifier-terser": "^5.1.1",
    "html-webpack-plugin": "^5.0.0",
    "json-typescript": "1.1.2",
    "less": "^4.1.1",
    "less-loader": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "mini-css-extract-plugin": "^1.3.6 || ^1.3.6 || ^2.0.0",
    "postcss": "^8.2.5",
    "postcss-loader": "^5.0.0 || ^6.0.0",
    "resolve-typescript-plugin": "^1.0.0-alpha.4",
    "style-loader": "^2.0.0 || ^3.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "tslib": "^2.1.0"
  },
  "bundledDependencies": [
    "clean-webpack-plugin",
    "css-loader",
    "cssnano",
    "ejs-loader",
    "extract-loader",
    "file-loader",
    "html-loader",
    "html-minifier-terser",
    "html-webpack-plugin",
    "less",
    "less-loader",
    "mini-css-extract-plugin",
    "postcss",
    "postcss-loader",
    "resolve-typescript-plugin",
    "style-loader",
    "terser-webpack-plugin"
  ],
  "peerDependencies": {
    "ts-loader": "^8.0.0 || ^9.0.0",
    "typescript": "^4.2.0",
    "webpack": "^5.21.2",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "4.0.1",
    "@softwareventures/prettier-config": "1.0.2",
    "@softwareventures/semantic-release-config": "2.0.0",
    "@types/cssnano": "4.0.1",
    "@types/html-minifier-terser": "5.1.2",
    "@types/html-webpack-plugin": "3.2.6",
    "@types/mini-css-extract-plugin": "2.0.1",
    "@types/node": "12.20.16",
    "@types/terser-webpack-plugin": "5.0.4",
    "@types/webpack-dev-server": "3.11.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.31.0",
    "prettier": "2.3.2",
    "semantic-release": "17.4.4",
    "typescript": "4.3.5",
    "webpack": "5.46.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures",
    "rules": {
      "@typescript-eslint/no-namespace": "off"
    }
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  },
  "snyk": true
}
